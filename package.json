{
  "name": "mywork",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production PROJECT_NAME=myWork tstl",
    "dev": "cross-env PROJECT_NAME=myWork tstl --watch",
    "prepare": "husky install",
    "clean": "rimraf dist/*"
  },
  "dependencies": {
    "@typescript-to-lua/language-extensions": "1.0.0",
    "awful-types": "1.5.10",
    "cross-env": "7.0.3",
    "lua-types": "2.13.0",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "typescript-to-lua": "1.10.1"
  },
  "devDependencies": {
    "@types/node": "18.11.9",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.42.1",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "tslib": "2.4.1"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --cache --fix",
      "prettier --write ."
    ]
  }
}
